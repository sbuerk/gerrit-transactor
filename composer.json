{
    "name": "sbuerk/gerrit-transactor",
    "description": "Transact and process Gerrit/GerritHub webhooke messages",
    "license": "GPL-2.0-or-later",
    "type": "project",
    "authors": [
        {
            "name": "Stefan BÃ¼rk",
            "email": "stefan@buerk.tech",
            "role": "Maintainer"
        }
    ],
    "homepage": "https://github.com/sbuerk/gerrit-transactor",
    "support": {
        "issues": "https://github.com/sbuerk/gerrit-transactor/issues"
    },
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "symfony/console": "6.3.*",
        "symfony/dotenv": "6.3.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.3.*",
        "symfony/runtime": "6.3.*",
        "symfony/yaml": "6.3.*"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.30.2",
        "friendsofphp/php-cs-fixer": "^3.16.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "APP\\": "src/",
            "App\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x.x-dev"
        },
        "symfony": {
            "allow-contrib": "false",
            "require": "6.3.*"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
